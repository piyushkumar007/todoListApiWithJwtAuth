{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "src",
        "paths": {
        "@utils/*": ["utils/*"],
        "@middlewares/*": ["middlewares/*"]
        }
    },
    "include": ["src/**/*","src/types/express.d.ts"],
    "exclude": ["node_modules"]
  }
  